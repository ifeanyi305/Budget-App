<button><%= link_to "back", groups_path  %></button>
<%= form_with(model: @group, url: groups_path, method: :post) do |form| %>
  <div class="form-group">
  <%= form.label :icon %>
  <%= form.text_field :icon, class: 'form-field', placeholder: "Image URL", autocomplete: "name" %>
  </div>

<div class="form-group">
  <%= form.label :name %>
  <%= form.text_field :name, class: 'form-field', autocomplete: "name" %>
</div>

<%= form.submit 'save', class: 'btn' %>
<% end %>
