<div><button class="save"><%= link_to "back", groups_path  %></button></div>
<div class="category_form">
    <%= form_with(model: @group, url: groups_path, method: :post) do |form| %>
      <div class="form-group">
      <%= form.label :icon, class: "label" %>
      <%= form.text_field :icon, class: 'form-field', placeholder: "Image URL", autocomplete: "name" %>
      </div>
    
    <div class="form-group">
      <%= form.label :name, class: "label" %>
      <%= form.text_field :name, class: 'form-field', autocomplete: "name" %>
    </div>
    
    <div class="category_btn_container"><%= form.submit 'save', class: 'new_category' %></div>
    <% end %>
</div>
